<body style="margin:0px;"  ng-app="mainApp" ng-controller="mainCtrl">
    <div id="topHover" class="topHover">
        <img src="/images/title.png" id="picTitle" class="picTitle" style="width:150px;margin-left:12px;margin-top: 18px;cursor: pointer" ng-click="toIndex()">
    </div>
    <div class="sidebar" id="sidebar" style="width: 260px;position: absolute;top: 64px;background-color: #aaa893">
        <div id="jquery-accordion-menu" style="width: 260px;" class="jquery-accordion-menu red">
            <div class="jquery-accordion-menu-header" id="form">
                <input class="sideInputBar">
            </div>
            <ul id="demo-list">

                <li>
                    <a href="#/ServicePublish"><img src="/images/icon/service.png" class="sideBarIcon">服务管理 </a>
                    <ul class="submenu">
                        <li ><a href="#/ServicePublish" >服务发布</a></li>
                        <li ><a href="#/ServiceInsert" >服务接入</a></li>
                        <li ><a href="#/MyServicePublish" >我发布的服务</a></li>
                        <li ><a href="#/MyServiceInsert" >我接入的服务</a></li>
                    </ul>
                </li>
                <li>
                    <a href="#/AppPublish"><img src="/images/icon/service.png" class="sideBarIcon">应用管理 </a>
                    <ul class="submenu">
                        <li ><a href="#/AppPublish" >应用发布</a></li>
                        <li ><a href="#/AppList" >应用列表</a></li>
                        <li ><a href="#/MyAppPublish" >我应用的服务</a></li>
                    </ul>
                </li>
                <li>
                    <a href="#/statistics"><img src="/images/icon/static.png" style="width:15px" class="sideBarIcon">统计概览</a>
                    <ul class="submenu">
                            <li><a href="#/statistics">总览</a></li>
                        <li><a href="#">服务</a></li>
                        <li><a href="#">API</a></li>
                        <li><a href="#">应用</a></li>
                        <li><a href="#">用户</a></li>
                    </ul>
                </li>
                <li  ng-click="change()">
                    <a href="#/NotificationCenter"  ng-click="change()"><img src="/images/icon/service.png" class="sideBarIcon">信箱 / 通知</a><span class="jquery-accordion-menu-label" ng-bind="messageNum"> </span>
                </li>
                <li><a href="#/userInfo"><img src="/images/icon/service.png" class="sideBarIcon">用户信息</a></li>
                <li><a href="#/" ng-click="logOff()"><img src="/images/icon/service.png" class="sideBarIcon"></i>退出登录 </a></li>
                <li><a href="#"><img src="/images/icon/service.png" class="sideBarIcon"></i>联系方式 </a></li>

            </ul>
            <div class="jquery-accordion-menu-footer" >
                HospitalServicePlatform
            </div>
        </div>
    </div>
    <div class="content" id="content" ui-view=""></div>
    <div id="rainDrop" class="rainDrop"></div>
