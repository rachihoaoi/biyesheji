<div class="title">服务接入
    <button class="saveService" ng-click="save()">接入</button>
    <div  class="searchBar">
        <input class="search" placeholder=""  ng-keyup="search()" ng-model="searchContent">
    </div>

</div>
<div class="line"></div>

<div class="publishArea">
    <div style="height:700px;width:1050px;display: inline-block;float: left;">
        <div class="topListHover">
            <div class="topListHoverItem">服务名称</div>
            <div class="topListHoverItem">服务提供商</div>
            <div class="topListHoverItem">请求方式</div>
            <div class="topListHoverItem">参数类型</div>
            <div class="topListHoverItem">点击进入详情页</div>
            <div class="topListHoverItem">是否接入</div>
        </div>
        <div id="about1" class="nano about1" style="width: 992px;background-color: white;margin-left: 2px;height: 700px">
            <div class="nano-content" style="padding-top: 0px">
                <p style="margin: 0px">
                    <div ng-repeat="service in serviceList" class="listLine" >
                        <div class="listContent" ng-bind="service.name"></div>
                        <div class="listContent" ng-bind="service.developer"></div>
                        <div class="listContent" ng-bind="service.requestMethod"></div>
                        <div class="listContent" ng-bind="service.paramType"></div>
                        <div class="listContent" ng-click="toInfo(service.id)" style="cursor: pointer">详情</div>
                        <div class="listContent" ><input type="checkbox" ng-click="selected(service.id)" style="margin-top: 12px;cursor: pointer;outline:none;"></div>
                    </div>
                </p>
            </div>
        </div>
