    <div class="osusumeBar">
        <lable class="osusumetitle">服务推荐</lable>
        <div class="osusumeContent">
            <div id="about1" class="nano about1" style="width: 400px;background-color: white;margin-left: 2px;height: 600px">
                <div class="nano-content" style="padding-top: 0px">
                    <p style="margin: 0px">
                    <div class="osusumeItem" ng-repeat="item in osusumeItem" ng-click="toInfo(item.id)">
                        <div style="height: 32px;width: 100%;">
                            <label style="line-height: 32px;overflow: hidden;cursor: pointer">服务名称：</label><label style="line-height: 32px;cursor: pointer;max-width: 150px;overflow: hidden;text-overflow: ellipsis" ng-bind="item.name">aaa</label>
                        </div>
                        <div style="height: 32px;width: 100%">
                            <label style="line-height: 32px;overflow: hidden;cursor: pointer;">服务提供商：</label><label style="line-height: 32px;cursor: pointer;max-width: 100px;overflow: hidden;text-overflow: ellipsis" ng-bind="item.developer">aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa</label>
                            <label style="line-height: 32px;overflow: hidden;cursor: pointer;float: right;" ng-bind="item.requestMethod">POST</label><label style="line-height: 32px;cursor: pointer;max-width: 150px;overflow: hidden;text-overflow: ellipsis;float: right;margin-right: 12px">请求方式：</label>
                        </div>
                        <div style="height: 32px;width: 100%">
                            <label style="line-height: 32px;overflow: hidden;cursor: pointer;">参数类型：</label><label style="line-height: 32px;cursor: pointer;max-width: 150px;overflow: hidden;text-overflow: ellipsis" ng-bind="item.bodyParamType">HEADER</label>
                        </div>
                    </div>

                    </p>
                </div>
            </div>
        </div>
