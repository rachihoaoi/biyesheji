            <div class="nano-content" style="padding-top: 0px">
                <p style="margin: 0px">
                <div ng-repeat="service in mailList" class="listLine" style="width: 1440px">
                    <div class="listContent" ng-bind="service.id"></div>
                    <div class="listContent" ng-bind="service.fromName"></div>
                    <div class="listContent" ng-bind="service.toName"></div>
                    <div class="listContent" ng-bind="service.time"></div>
                    <div class="listContent" ng-bind="service.title"></div>
                    <div class="listContent" style="overflow: hidden; white-space: nowrap; text-overflow: ellipsis;width:320px;" ng-bind="service.message"></div>
                    <div class="listContent" ng-click="toInfo(service.id)"  style="cursor: pointer">详情</div>

                    <div class="listContent" ><input type="checkbox" ng-click="selected(service.id);addDeveId(service.developerID)" style="margin-top: 12px;cursor: pointer;outline:none;"></div>
                </div>
                </p>
            </div>
        </div>
